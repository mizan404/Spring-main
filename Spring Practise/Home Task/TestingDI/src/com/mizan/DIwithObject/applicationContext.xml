<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="address" class="com.mizan.DIwithObject.Address">
        <constructor-arg value="dhaka">        </constructor-arg>
        <constructor-arg value="Dhaka">        </constructor-arg>
        <constructor-arg value="Bangladesh">        </constructor-arg>
        
    </bean>
    <bean id="employee" class="com.mizan.DIwithObject.Employee">
        <constructor-arg value="404" type="int>        </constructor-arg>
        <constructor-arg value="Mizan">        </constructor-arg>
        <constructor-arg  > 
            <ref bean="address"/>       
        </constructor-arg>
        
    </bean>
    
    
</beans> 
